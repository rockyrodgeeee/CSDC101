#include <iostream> 
#include <cstring>
using namespace std;


int main(){

char super[10] = "Spiderman";
char alter[10] = "Spiderman";

if(strcmp(super, alter) == 0){
    cout << "The identity is revealed" << endl;
    
}

else
cout<< "Mismatch"<<endl;

return 0;
}


#include <iostream> 
#include <cstring>
using namespace std;


int main(){

string superName = "Homelander";

cout<< "Enter SuperHeroname"<< endl;
getline(cin, superName);

cout<< "The super hero name is" <<superName<< endl;

cout<<superName[0]<< endl;
cout<<superName[1]<< endl;
cout<<superName[2]<< endl;
cout<<superName[3]<< endl;
cout<<superName[4]<< endl;
cout<<superName[5]<< endl;

return 0;
}

#include <iostream> 
#include <cstring>
using namespace std;


int main(){

string firstName = "Ben"

cout<< "Before Append" << endl;
firstName.append("idict Eborde");

cout<< "After Append" << endl;
cout<<firstName << endl;


return 0;
}

#include <iostream> 
#include <cstring>
using namespace std;


int main(){

string Name = "Ben";
string Winner = "Team korea";

if(Name == Winner){
    cout<< "Paldo"<<endl;
    
}
else
cout<<"Talo"<< endl;
return 0;
}

