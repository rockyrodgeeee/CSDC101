#include <iostream>
using namespace std;

int main()
{
   for(int row = 1;  row <=10; row++){
  
  for(int column = 1;  column <=10; column++){
      cout<<"*";
      
  }
  cout<<endl;
}
    return 0; 
}

example 2:
#include <iostream>
using namespace std;

int main()
{
   for(int row = 1;  row <=10000000000; row++){
  
  cout<<"row number:"<<row<<" ";
  for(int column = 1;  column <=10; column++){
      cout<<column<< " ";
      
  }
  cout<<endl;
}
    return 0; 
}

triangle:
#include <iostream>
using namespace std;

int main()
{
   for(int row = 1;  row <=10; row++){
  for(int column = 1;  column <=row; column++){
      cout<< "* ";
      
  }
  cout<<endl;
}
    return 0; 
}

reverse triangle:
#include <iostream>
using namespace std;

int main()
{
   for(int row = 10;  row >= 1; row--){
  for(int column =  row; column > 0; column--){
      cout<< "| ";
      
  }
  cout<<endl;
}


 for(int row = 1; row <=10; row++){
  for(int column = 1;  column <= row; column++){
      cout<< "| ";
      
  }
  cout<<endl;
}

    return 0; 
}

HALF DIAMOND:
#include <iostream>
using namespace std;

int main()
{
   for(int row1 = 0;  row1 <= 10; row1++){
  for(int column1 = 1; column1 <= row1; column1++){
      cout<< "| ";
      
  }
  cout<<endl;
}


 for(int row2 = 10; row2 >=1; row2--){
  for(int column2 = 1;  column2 <= row2; column2++){
      cout<< "| ";
      
  }
  cout<<endl;
}

    return 0; 
}
