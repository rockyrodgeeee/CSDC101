#include <iostream>
using namespace std;

int main()
{
   for(int row = 1;  row <=10; row++){
  
  for(int column = 1;  column <=10; column++){
      cout<<"*";
      
  }
  cout<<endl;
}
    return 0; 
}

example 2:
#include <iostream>
using namespace std;

int main()
{
   for(int row = 1;  row <=10000000000; row++){
  
  cout<<"row number:"<<row<<" ";
  for(int column = 1;  column <=10; column++){
      cout<<column<< " ";
      
  }
  cout<<endl;
}
    return 0; 
}

triangle:
#include <iostream>
using namespace std;

int main()
{
   for(int row = 1;  row <=10; row++){
  for(int column = 1;  column <=row; column++){
      cout<< "* ";
      
  }
  cout<<endl;
}
    return 0; 
}
