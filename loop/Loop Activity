#include <iostream>
using namespace std;

int main()
{
   for(int row = 10;  row >= 1; row--){
  for(int column =  row; column > 0; column--){
      cout<< "| ";
      
  }
  cout<<endl;
}


 for(int row = 1; row <=10; row++){
  for(int column = 1;  column <= row; column++){
      cout<< "| ";
      
  }
  cout<<endl;
}

    return 0; 
}
