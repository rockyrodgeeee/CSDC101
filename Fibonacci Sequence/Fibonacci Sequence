Harmonic Sequence

#include<iostream>
using namespace std;
int main(){
    
    float numerator, demonimator = 3, n, condition = 1;
    
    cout<< "Enter number of terms in sequence: ";
    cin>>n;
    while(condition <= n){
        cout<< "1/"<< demonimator << endl;
        demonimator = demonimator + 3;
        condition++; 
        
    }
    
    
    return 0;
}
